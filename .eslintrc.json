{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [ "eslint:recommended", "airbnb-base" ],
  "rules": {
    "indent": [ "error", 2 ],
    "linebreak-style": [ "error", "unix" ],
    "quotes": [ "error", "single" ],
    "semi": [ "error", "always" ],
    "no-restricted-globals": 0,
    "max-len": 0
  }
}
